    "type": "library",
    "license": "MIT",
    "keywords": [
        "utilities",
        "utils",
        "arrays",
        "optional",
        "java optional"
    ],
    "authors": [
        {
            "name": "Freer",
            "email": "support@davidfreer.com"
        }
